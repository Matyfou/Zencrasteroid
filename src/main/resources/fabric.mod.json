{
  "schemaVersion": 1,
  "name": "Asteroid",
  "id": "meteor-client",
  "version": "${version}",
  "description": "Utility client with only the utilities. Forked from Meteor Client by Moxvallix. Meteor Client by MineGame159, squidoodly and seasnail",
  "authors": [
    "MineGame159",
    "squidoodly",
    "seasnail",
    "Moxvallix"
  ],
  "contact": {
    "homepage": "https://github.com/optipack/asteroid",
    "issues": "https://github.com/optipack/asteroid/issues",
    "sources": "https://github.com/optipack/asteroid"
  },
  "license": "GPL-3.0",
  "icon": "assets/meteor-client/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "meteordevelopment.meteorclient.MeteorClient"
    ]
  },
  "mixins": [
    "meteor-client.mixins.json",
    "meteor-client-baritone.mixins.json",
    "meteor-client-indigo.mixins.json",
    "meteor-client-sodium.mixins.json",
    "meteor-client-canvas.mixins.json",
    "meteor-client-lithium.mixins.json",
    "meteor-client-indium.mixins.json",
    "meteor-client-viafabricplus.mixins.json"
  ],
  "accessWidener": "meteor-client.accesswidener",
  "custom": {
    "meteor-client:devbuild": "${devbuild}",
    "meteor-client:color": "145,61,226",
    "meteor-client:commit": "${commit}",
    "modmenu": {}
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["1.20.5", "${minecraft_version}"],
    "fabricloader": ">=${loader_version}"
  },
  "breaks": {
    "optifabric": "*",
    "feather": "*",
    "origins": "*",
    "wurst": "*",
    "sodium": "<0.5.8",
    "morechathistory": "*"
  }
}
